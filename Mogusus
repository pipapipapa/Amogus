import os
 
 
def f(i, count):
    if i == 15:
        return 0
    else:
        os.mkdir(f"ttt{count * 2}")
        os.chdir(f"ttt{count * 2}")
        f(i + 1, count * 2)
        os.chdir("..")
        os.mkdir(f"ttt{count * 2 + 1}")
        os.chdir(f"ttt{count * 2 + 1}")
        f(i + 1, count * 2 + 1)
        os.chdir("..")
 
os.mkdir("ttt0")
os.chdir("ttt0")
f(0, 1)
os.chdir("..")
os.chdir("ttt0")
os.system("rd /s /q .")
